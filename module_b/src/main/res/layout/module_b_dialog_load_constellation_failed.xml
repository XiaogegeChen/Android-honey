            app:message_dialog_cancel="@string/module_b_dialog_load_constellation_failed_exit"
            app:message_dialog_confirm="@string/module_b_dialog_load_constellation_failed_retry"
            app:message_dialog_content="@string/module_b_dialog_load_constellation_failed_content"/>
</FrameLayout>